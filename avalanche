local Library = loadstring(game:HttpGet('https://raw.githubusercontent.com/Loco-CTO/UI-Library/main/VisionLibV2/source.lua'))()

local Window = Library:Create({
	Name = "Kyoichi", -- String
	Footer = "Report bugs at Discord", -- String
	ToggleKey = Enum.KeyCode.LeftAlt, -- Enum.KeyCode
	LoadedCallback = function()
		print("Kyoichi Loaded Succesfully. Any suggestions or bugs? discord.gg/t9FXbh6HgY")
	end,
	
	KeySystem = false, -- Boolean
	Key = "ofcPMj6ikR8nnAHlI93ZPtBn", -- String
	MaxAttempts = 5, -- Integer
	DiscordLink = "https://discord.gg/t9FXbh6HgY", -- String (Set it to nil if you do not have one, the button will not pop out)
	ToggledRelativeYOffset = 5 -- Number (Y Offset from bottom of your screen. Set it to nil if you want it to be centred)
})


local Tab = Window:Tab({
	Name = "Simulator Games", -- String
	Icon = "rbxassetid://6678908798", -- String
	Color = Color3.new(1, 0, 0) -- Color3
})

local Section = Tab:Section({
	Name = "Simulator Games listed here." -- String
})

